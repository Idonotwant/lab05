{
  "version": 2,
  "builds": [
    {
      "src": "/src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["../frontend/dist/**"],
        "buildCommand": "prisma generate && yarn build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
